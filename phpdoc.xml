<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
        configVersion="3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="https://www.phpdoc.org"
        xsi:noNamespaceSchemaLocation="data/xsd/phpdoc.xsd"
>
    <title>VIP Go mu-plugins</title>
    <paths>
        <output>docs</output>
    </paths>
    <version number="3.0.0">
        <folder>latest</folder>
        <api>
            <source dsn="vip-go-mu-plugins/">
                <path>000-vip-init.php</path>
                <path>lib/proxy/ip-forward.php</path>
                <path>vip-cache-manager</path>
                <path>cache/class-vary-cache.php</path>
                <path>vip-helpers</path>
                <path>cron-control</path>
                <path>drop-ins/object-cache/object-cache.php</path>
                <path>gutenberg-ramp</path>
                <path>http-concat</path>
                <path>search/es-wp-query</path>
            </source>
            <output>api</output>
            <ignore hidden="true" symlinks="true">
                <path>vip-helpers/vip-deprecated.php</path>
            </ignore>
            <extensions>
                <extension>php</extension>
            </extensions>
            <include-source>true</include-source>
            <ignore-tags>
                <ignore-tag>extends</ignore-tag>
                <ignore-tag>implements</ignore-tag>
            </ignore-tags>
            <default-package-name>phpDocumentor</default-package-name>
        </api>
    </version>
    
</phpdocumentor>